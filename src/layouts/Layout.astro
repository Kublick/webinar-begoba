---
export interface Props {
  title: string;
}

import '../styles/global.css';

const { title } = Astro.props as Props;
---

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <link rel="icon" type="image/x-icon" href="/icon-192x192.png" />
  <title>{title}</title>
</head>

<body>
  <slot />
</body>

</html>